# top-most .editconfig file
root = true

# set default charset
charset = utf-8

# 4 space indentation
indent_style = space
ident_size = 4

# unix-style newlines
end_of_line = lf

# add newline ending for every file
insert_final_newline = true

# remove any whitespace characters
# preceding newline characters
trim_trailing_whitespace = true
